
<h2>Questions</h2>
<% if @survey.questions().any? %>
  <ul>
    <% @survey.questions().each do |question| %>
      <li><%= question.description() %></a>
      <a href="/questions/<%=question.id() %>/edit">Edit</a></li>
    <%end%>
  </ul>
<% else %>
  <p> There are no questions yet! </p>
<% end %>
<a href="/">Return Home</a>
